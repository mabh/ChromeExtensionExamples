{
  "manifest_version": 2,
  "name": "MWS",
  "description": "This extension demonstrates a browser action with MWS",
  "version": "1.0",
  "permissions": ["tabs"],
  "content_scripts": [ {
     "js": [ "contentscript1.js" ],
     "matches": ["http://*/*", "https://*/*"],
	 "run_at": "document_end"
  }],
  "browser_action": {
    "default_icon": "sag.png",
	"default_popup": "popup.html"
  }
}